		<script src='js/jquery.js'></script>
		<script src='js/bootstrap.min.js'></script>
		<script src='js/script.js'></script>
		<script src='js/jquery.parallax-1.1.3.js'></script>
  </head>
  <body id='dev'>

		{% set page = '/contacts' %}

	<wrapper class="container-fluid">

		<section id='scaling'>

			<div class="type" id="mob">
				<div class="screen" id='m320'>
					<div class="cent">
						<span>XS - 320px</span>
						<object type="text/html" data="{{page}}" ></object>
					</div>
				</div>
				<div class="screen" id='m576'>
					<div class="cent">
						<span>SM - 576px</span>
						<object type="text/html" data="{{page}}" ></object>
					</div>
				</div>
			</div>

			<div class="type" id="middle">
				<div class="screen" id='m768'>
					<div class="cent">
						<span>MD - 768px</span>
						<object type="text/html" data="{{page}}" ></object>
					</div>
				</div>
				<div class="screen" id='m1024'>
					<div class="cent">
						<span>LG - 1024px</span>
						<object type="text/html" data="{{page}}" ></object>
					</div>
				</div>
			</div>

			<div class="type" id="desktop">
				<div class="screen" id='m1440'>
					<span>XL - 1440px</span>
					<object type="text/html" data="{{page}}" ></object>
				</div>
				<div class="screen" id='m1920'>
					<span>XXL - 1920px</span>
					<object type="text/html" data="{{page}}" ></object>
				</div>
				<div class="screen" id='m2560'>
					<span>MXXL - 2560px</span>
					<object type="text/html" data="{{page}}" ></object>
				</div>
			</div>

		</section>
  </wrapper>

  </body>
</html>
