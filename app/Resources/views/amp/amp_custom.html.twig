{% block headerMeta %}
	<style amp-custom>
		body {
			margin: 0;
			/*font-family: 'Georgia', Serif;*/
			padding-top: 52px;
			font-family: -apple-system,BlinkMacSystemFont,"Segoe UI",Roboto,Oxygen-Sans,Ubuntu,Cantarell,"Helvetica Neue",sans-serif;
			-moz-osx-font-smoothing: grayscale;
			-webkit-font-smoothing: antialiased;
		}

		@media (max-width: 290px) {
			.call-us {
				padding: 7px 4px;
				font-size: 20px;
			}

			body > header > button {
				font-size: 20px;
				padding-left: 4px;
				padding-right: 4px;
			}
			#resp-menu-word {
				display: none;
			}
		}

		@media (min-width: 291px) and (max-width: 332px) {
			.call-us {
				padding: 7px 8px;
				font-size: 20px;
			}

			body > header > button {
				font-size: 20px;
			}
		}

		@media (min-width: 333px) {
			.call-us {
				padding: 4px 8px;
				font-size: 24px;
			}

			body > header > button {
				font-size: 24px;
			}
		}

		.call-us {
			font-family: BlinkMacSystemFont;
			float: right;
			display: inline-block;
			text-align: right;
			vertical-align: top;
		}

		.call-us > a {
			color: #c12c21;
			text-decoration: none;
		}

		body > header > button {
			/*display: inline-block;*/
			color: white;
		}

		.ad-container {
			display: flex;
			justify-content: center;
		}

		.content-container p {
			line-height: 24px;
		}

		header,
		.static-header,
		.article-body {
			padding: 15px;
		}

		body > header {
			position: fixed;
			top: 0;
			left: 0;
			right: 0;
			/*height: 42px;*/
			background: black;
			z-index: 10;
			padding: 8px 15px;
			/*transform: translateX(20px);*/
		}

		body > .static-header {
			/*min-height: 64px;*/
			background: #58b6f4;
			z-index: 10;
			padding: 2px 0px;
		}

		@media (max-width: 284px) {
			.address {
				font-size: 7px;
			}
		}

		@media (min-width: 285px) and (max-width: 310px) {
			.address {
				font-size: 9px;
				line-height: 14px;
			}
		}
		@media (min-width: 311px) and (max-width: 335px) {
			.address {
				font-size: 10px;
				line-height: 14px;
			}
		}
		@media (min-width: 336px) and (max-width: 360px) {
			.address {
				font-size: 11px;
				line-height: 15px;
			}
		}
		@media (min-width: 361px) and (max-width: 500px) {
			.address {
				font-size: 12px;
				line-height: 16px;
			}
		}
		@media (min-width: 501px) {
			.address {
				font-size: 14px;
				line-height: 18px;
			}
		}
		.address {
			display: inline-block;
			padding: 4px 8px;
			font-family: BlinkMacSystemFont;
		}

		.logo {
			display: inline-block;
			padding: 4px 8px;
			border-right: 1px solid #000;
			width: 35%;
		}

		.float {
			display: none;
			background: yellow;
			border: 5px solid black;
			border-radius: 50%;
			opacity: 0.7;
			position: fixed;
			z-index: 1000;
			top: 100px;
			right: 10px;
			width: 40px;
			height: 40px;
		}

		/* In the shadow-doc mode, the parent shell already has header and nav. */
		.amp-shadow header {
			display: none;
		}

		.lightbox {
			background: #222;
		}

		.full-bleed {
			margin: 0 -15px;
		}

		.lightbox-content {
			position: absolute;
			top: 0;
			left: 0;
			right: 0;
			bottom: 0;

			display: flex;
			flex-direction: column;
			flex-wrap: nowrap;
			justify-content: center;
			align-items: center;
		}

		.lightbox-content p {
			color: #fff;
			padding: 15px;
		}

		.lightbox amp-img {
			width: 100%;
		}

		figure {
			margin: 0;
		}

		figcaption {
			color: #6f757a;
			padding: 15px 0;
			font-size: .9em;
		}

		.author {
			display: flex;
			align-items: center;

			background: #f4f4f4;
			padding: 0 15px;

			font-size: .8em;

			border: solid #dcdcdc;
			border-width: 1px 0;
		}

		.header-time {
			color: #a8a3ae;
			font-family: 'Roboto';
			font-size: 12px;
		}

		.author p {
			margin: 5px;
		}

		.byline {
			font-family: 'Roboto';
			display: inline-block;
		}

		.byline p {
			line-height: normal;
		}

		.byline .brand {
			color: #6f757a;
		}

		.standfirst {
			color: #6f757a;
		}

		.mailto {
			text-decoration: none;
		}

		#author-avatar {
			margin: 10px;
			border: 5px solid #fff;
			width: 50px;
			height: 50px;
			border-radius: 50%;
		}

		h1 {
			margin: 5px 0;
			font-weight: normal;
		}

		footer {
			display: flex;
			align-items: center;
			justify-content: center;
			height: 226px;
			background: #f4f4f4;
		}

		hr {
			margin: 0;
		}

		amp-app-banner {
			background: #f4f4f4;
			padding: 10px;
			box-shadow: 0 2px 5px 0 #000;
		}

		amp-app-banner .content {
			display: flex;
		}

		amp-img {
			/*background-color: #f4f4f4;*/
		}

		.slot-fallback {
			padding: 16px;
			background: yellow;
		}

		amp-app-banner amp-img {
			margin-right: 15px
		}

		amp-app-banner h3 {
			margin: 0;
		}

		amp-app-banner p {
			font-size: 14px;
			margin: 3px 0;
		}

		amp-app-banner a {
			padding: 8px;
			margin-right: 10px;
		}
		amp-app-banner .actions {
			text-align: right;
			font-size: 14px;
		}

		amp-sidebar {
			width: 150px;
		}

		nav li {
			list-style: none;
			margin-bottom: 10px;
		}

		nav li a {
			text-decoration: none;
			color: #666666;
		}

		.free-case-form {
			padding: 0;
			margin: 0;
			flex: none;
			width: auto;
			/*max-width: 1280px;*/
			background-color: #aad8f5;
		}

		form {
			display: -webkit-flex;
			display: -ms-flexbox;
			display: flex;
			flex-flow: row wrap;
			-webkit-flex-direction: row;
			-ms-flex-direction: row;
			flex-direction: row;
			padding: 16px;
			-webkit-flex-grow: 1;
			-ms-flex-positive: 1;
			flex-grow: 1;
			margin: 0;
			color: inherit;
		}

		.form-title {
			text-align: center;
			padding: 8px 8px 0;
			color: #c12c21;
			font-size: 18px;
			text-transform: uppercase;
			-webkit-margin-before: 0px;
			-webkit-margin-after: 0px;
		}

		form.amp-form-submit-success [submit-success],
	  form.amp-form-submit-error [submit-error]{
	    margin-top: 16px;
	  }
	  form.amp-form-submit-success [submit-success] {
	    color: green;
	  }
	  form.amp-form-submit-error [submit-error] {
	    color: red;
	  }
	  form.amp-form-submit-success.hide-inputs > input {
	    display: none
	  }
	  .horizontal-display {
	    display: flex;
	    align-items: center;
	  }
	  .other-input {
	    margin: 0 16px;
	  }

		a.input {
			width: 100%;
		}

		input[type=date], input[type=tel], input[type=email], input[type=text] {
			display: block;
			font-family: Helvetica,Arial,sans-serif;
			margin: 0 0 16px;
			width: 100%;
			text-align: left;
			color: inherit;
			outline: 0;
		}
		.data-input {
			border: none;
			border-bottom: 1px solid rgba(0,0,0,.12);
			font-size: 16px;
			margin-bottom: 16px;
			padding: 4px 0;
			color: inherit;
			outline: 0;
			background: 0 0;
		}
		.button, button {
			background: 0 0;
			border: none;
			border-radius: 2px;
			height: 36px;
			margin: 0;
			min-width: 64px;
			padding: 0 16px;
			display: inline-block;
			font-family: Roboto,Helvetica,Arial,sans-serif;
			font-size: 14px;
			font-weight: 500;
			text-transform: uppercase;
			letter-spacing: 0;
			will-change: box-shadow;
			transition: box-shadow .2s cubic-bezier(.4,0,1,1),background-color .2s cubic-bezier(.4,0,.2,1),color .2s cubic-bezier(.4,0,.2,1);
			outline: 0;
			text-align: center;
			line-height: 36px;
		}
		.anchor, .button, button {
			position: relative;
			vertical-align: middle;
		}
		.button, body a, button {
			cursor: pointer;
			text-decoration: none;
		}

		.button-primary {
			color: #fff;
			background-color: #c12c21;
			box-shadow: 0 2px 2px 0 rgba(0,0,0,.14), 0 3px 1px -2px rgba(0,0,0,.2), 0 1px 5px 0 rgba(0,0,0,.12);
		}

		.amp-nav {
			text-align: center;
			background: #91d3fe;
		}

		.amp-nav-current {
			margin: 0;
			padding: 12px;
			font-size: 20px;
			text-transform: uppercase;
		}

		.landing-quote {
			font-size: 20px;
			line-height: 28px;
			padding: 12px;
		}
		.landing-quote > strong {
			font-size: 24px;
			line-height: 32px;
			text-align: right;
		}

		h1#blog-h1 {
			text-align: center;
			font-size: 32px;
		}

		.amp-practice-h1 {
			background-color: rgb(207, 230, 255);
		}

		a.amp-practice-a {
			color: #fff;
		}

		.amp-practice-h1 > h1 {
			text-align: center;
			font-size: 26px;
			line-height: 32px;
			font-weight: 200;
			margin: 0;
			padding: 4px 0;
		}

		ul.amp-practice-ul {
			margin: 0;
			padding: 0;
			width: 100%;
		}

		ul.amp-practice-ul > li {
			list-style-type: none;
			display: inline-block;
			width: 50%;
			min-height: 140px;
			padding: 70px 0 10px;
			margin: 0 -2px;
			text-align: center;
			vertical-align: middle;
			font-size: 36px;
			font-weight: 600;
		}

		li.amp-practice-criminal {
			background-color: #beeab7;
		}

		li.amp-practice-family {
			background-color: #91d3fe;
		}

		li.amp-practice-appeal {
			background-color: #9195fe;
		}

		li.amp-practice-civil {
			background-color: #f5aa99;
		}

		footer {
			height: 120px;
			background: #424788;
		}

		footer > .brand-logo > a {
			color: #fff;
		}

	</style>
{% endblock %}
